{
  "functions": {
    "src/app/api/upload/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/analyze-brand/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/onboarding/upload-photos/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/posts/worker/route.ts": {
      "maxDuration": 120
    },
    "src/app/api/posts/generate-assets/route.ts": {
      "maxDuration": 120
    },
    "src/app/api/process/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/process-klap/route.ts": {
      "maxDuration": 60
    },
    "src/app/api/worker/klap/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/process-transcription/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/apply-subtitles/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-thumbnail/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-images/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-blog/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-social/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-caption/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/generate-summary/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/projects/[id]/process/route.ts": {
      "maxDuration": 300
    },
    "src/app/api/social/publish-scheduled/route.ts": {
      "maxDuration": 60
    }
  },
  "headers": [
    {
      "source": "/api/upload",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "POST, OPTIONS"
        },
        {
          "key": "Access-Control-Allow-Headers",
          "value": "Content-Type, Authorization"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/help",
      "destination": "/docs"
    }
  ],
  "crons": [
    {
      "path": "/api/social/publish-scheduled",
      "schedule": "*/5 * * * *"
    },
    {
      "path": "/api/cron/klap",
      "schedule": "*/5 * * * *"
    }
  ]
} 